journal = {Mathematical Geology},
keywords = {closed arrays,compositional data,geochemistry,principal-component analysis,subcomposition},
number = {6},
pages = {617--635},
title = {{Reducing the dimensionality of compositional data sets}},
volume = {16},
year = {1984}
}

